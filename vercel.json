{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      { "src": "/atlassian-connect.json", "dest": "atlassian-connect.json" },
      { "src": "/(.*)", "dest": "app.py" }
    ]
  }  